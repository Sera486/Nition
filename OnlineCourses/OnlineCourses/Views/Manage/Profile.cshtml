@model ApplicationUser
@{
    ViewData["Title"] = "Профіль";
}

<h2>@ViewData["Title"].</h2>
<p class="text-success">@ViewData["StatusMessage"]</p>

<body>
<div class="container=fluid" id="InfoBack" style="background-image: url(img/InfoPage.jpg) ;">
    <div class="row" >
        <br>
        <div class="col-xs-1"></div>
        <div class="col-sm-1" style=" margin-left: 30;">
            <img src="img/150.jpg" class="img" alt="user photo" width="100" height="100">
        </div>
        <div class="col-xs-4">
            <h1 >Логін користувача:@Model.UserName</h1>
        </div>
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <br>
            <p style="font: 15pt sans-serif;">@Model.FullName</p>
        </div>
        <div class="col-lg-12">
            <br>
            <p style="font: 15pt sans-serif;">@Model.Email</p>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-11">
            <div id="accordion" class="panel-group">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4 class="panel-title">
                            <a href="#collapse-1" data-toggle="collapse">Курси</a>
                        </h4>
                    </div>
                    <div id="collapse-1" class="panel-collapse collapse">

                    </div>
                </div>
                <div class="panel panel-default">
                        
                </div>
                <div class="panel panel-default"></div>
            </div>
        </div>
            
    </div>
</div>
<!-- jQuery (потрібно для JavaScript плагінів Bootstrap) -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<!-- Підключення мінімізованої збірки всіх плагінів jQuery. Можна підключати додаткові плагіни, якщо потрібно -->
</body>
